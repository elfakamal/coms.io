{
  "name": "coms.io",
  "version": "1.0.0",
  "description": "communications",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --inline",
    "build": "NODE_ENV=production webpack --progress && babel src/middleware -d ./dist/middleware",
    "build-and-optimize": "NODE_ENV=production webpack --progress --optimize-minimize --optimize-occurence-order",
    "test:dev": "mocha --colors --compilers js:babel-core/register --require ./test/config.js 'test/**/*.@(js|jsx)'",
    "test": "mocha --colors --compilers js:babel-core/register --require ./test/config.js 'test/**/*.@(js|jsx)' -R xunit > mocha-reports.xml",
    "test:watch": "npm run test:dev -- --watch --watch-extensions js",
    "api": "json-server ./src/api/index.js"
  },
  "keywords": [
    "communications"
  ],
  "author": "Kamal Farsaoui <elfa.kamal@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-destructuring": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "latest",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "faker": "^3.1.0",
    "file-loader": "^0.9.0",
    "jsdom": "^9.8.3",
    "json-server": "^0.9.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.2.0",
    "mockery": "^2.0.0",
