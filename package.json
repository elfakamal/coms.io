{
  "name": "coms.io",
  "version": "1.0.0",
  "description": "communications",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --inline",
    "build": "NODE_ENV=production webpack --progress && babel src/middleware -d ./dist/middleware",
    "build-and-optimize": "NODE_ENV=production webpack --progress --optimize-minimize --optimize-occurence-order",
    "test:dev": "mocha --colors --compilers js:babel-core/register --require ./test/config.js 'test/**/*.@(js|jsx)'",
    "test": "mocha --colors --compilers js:babel-core/register --require ./test/config.js 'test/**/*.@(js|jsx)' -R xunit > mocha-reports.xml",
    "test:watch": "npm run test:dev -- --watch --watch-extensions js"
  },
  "keywords": [
    "communications"
  ],
  "author": "Kamal Farsaoui <elfa.kamal@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.14.2",
    "moment": "^2.14.1",
    "radium": "^0.18.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-localstorage": "^0.4.1",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "redux-saga": "^0.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "^0.9.0",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.2",
    "mockery": "^1.7.0",
    "react-addons-test-utils": "^15.3.0",
    "react-hot-loader": "^1.3.0",
    "react-tools": "^0.13.3",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "sinon": "^1.17.5",
    "url-loader": "^0.5.7",
    "vhost": "^3.0.2",
